[1m+++ b/src/hooks/useIdle.tsx[m
[36m@@ -0,0 +1,33 @@[m
[32m+[m[32mimport {useState } from 'react';[m
[32m+[m[32mimport {useIdleTimer} from 'react-idle-timer'[m
[32m+[m
[32m+[m[32minterface UseIdleProps{[m
[32m+[m[32m  onIdle: () => void;[m
[32m+[m[32m  idleTime: number | undefined;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mexport const useIdle = ({[m
[32m+[m[32m  //ADD THE onIdle AND idleTime props[m
[32m+[m[32m  onIdle,[m
[32m+[m[32m  idleTime=1[m
[32m+[m[32m}:UseIdleProps) => {[m
[32m+[m[32m  const [isIdle, setIsIdle] = useState(false);[m
[32m+[m
[32m+[m[32m  const handleOnIdle = (event:any)=> {[m
[32m+[m[32m    setIsIdle(true);[m
[32m+[m[32m    console.log("user is idle", event)[m
[32m+[m[32m    console.log("Last Active", getLastActiveTime())[m
[32m+[m[32m    onIdle()[m
[32m+[m[32m  }[m
[32m+[m[32m  const {getRemainingTime, getLastActiveTime} = useIdleTimer({[m
[32m+[m[32m    timeout: 1000 * 60 * idleTime,[m
[32m+[m[32m    onIdle: handleOnIdle,[m
[32m+[m[32m    debounce: 500[m
[32m+[m[32m  })[m
[32m+[m
[32m+[m[32m  return {[m
[32m+[m[32m    getRemainingTime,[m
[32m+[m[32m    getLastActiveTime,[m
[32m+[m[32m    isIdle[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
